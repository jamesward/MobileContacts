<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
    xmlns:fb="http://ns.adobe.com/flashbuilder/2009">

    <fx:Metadata>
    <![CDATA[ 
        [HostComponent("spark.components.Button")]
    ]]>
    </fx:Metadata> 
    
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>

    <!-- background -->
    <s:Rect left="0" top="1" right="0" bottom="2" minWidth="4" minHeight="200"
			bottomLeftRadiusX="5" bottomRightRadiusX="5" bottomLeftRadiusY="5" bottomRightRadiusY="5"
			topLeftRadiusX="5" topRightRadiusX="5" topLeftRadiusY="5" topRightRadiusY="5">
        <s:stroke>
            <s:SolidColorStroke color="0x5C5C5C" weight="1"/>
        </s:stroke>
        <s:fill>
            <s:SolidColor color="0x505050" />
        </s:fill>
    </s:Rect>

</s:SparkSkin>